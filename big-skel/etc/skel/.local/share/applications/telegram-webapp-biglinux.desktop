#!/usr/bin/env xdg-open
[Desktop Entry]
Version=1.0
Terminal=false
Type=Application
Name=Telegram
Exec=/usr/bin/biglinux-webapp --class="webz.telegram.org,Chromium-browser" --profile-directory=Default --app=https://webz.telegram.org
Icon=/usr/share/icons/hicolor/128x128/apps/telegram-webapp.png
StartupWMClass=webz.telegram.org
Categories=Webapps;
